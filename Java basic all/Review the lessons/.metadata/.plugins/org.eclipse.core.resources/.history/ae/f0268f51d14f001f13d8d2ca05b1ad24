package Exception;

public class ExceptionDemo {
	public static void main(String[] arqs) {
		System.out.println("First line");
		int a = 5;
		int b = 0;
		try {
			double x = a/b;
		}catch (ArithmeticException e) {
			System.out.println("Error arithmetic!");
		}
		System.out.println("Seond line");
	}
}
