package Map;

import java.util.HashMap;
import java.util.Map;

public class MapDeom {
	public static void main(String[] arqs) {
		//mango 10.50$
		//banana 12.0$
		//apple 25$
		Map<String, Double> listFruit = new HashMap<>();
		listFruit.put("Mango", 10.50);
		listFruit.put("Banana", 12.0);
		listFruit.put("Apple", 25.0);
		System.out.println(listFruit);
		System.out.println("====================================");
		Double BananaPrice = listFruit.get("Banana");
		System.out.println(BananaPrice);
	}
}
