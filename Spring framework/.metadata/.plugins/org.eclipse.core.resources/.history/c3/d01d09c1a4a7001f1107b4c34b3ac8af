package com.piseth.java.school;

import org.springframework.stereotype.Component;

@Component
public class Student {
	
	private Subject subject;
	
	public void init() {
		System.out.println("Student created");
	}
	
	public void destroy() {
		System.out.println("Student destroyed");
	}
	
	public void displau() {
		subject.setSub();
	}
}
